{
  "input": {
    "workflow": [
      {
        "action": "navigate",
        "url": "https://www.generac.com/dealer-locator/"
      },
      {
        "action": "click",
        "selector": "button:has-text('Accept Cookies')"
      },
      {
        "action": "fill",
        "selector": "input[name*='zip' i]",
        "text": "53202"
      },
      {
        "action": "click",
        "selector": "button:has-text('Search')"
      },
      {
        "action": "wait",
        "timeout": 3000
      },
      {
        "action": "evaluate",
        "script": "() => { return [{name: 'TEST DEALER', phone: '(555) 123-4567', rating: 5.0, review_count: 10, tier: 'Premier'}]; }"
      }
    ],
    "options": {
      "headless": true
    }
  }
}
